<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="github.chenupt.sample" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activities.NormalListActivity"
            android:label="NormalListActivity">
        </activity>
        <activity
            android:name=".activities.AAListActivity_"
            android:label="AAListActivity">
        </activity>
        <activity
            android:name=".activities.ViewPagerActivity"
            android:label="ViewPagerActivity">
        </activity>
        <activity
            android:name=".activities.NormalHolderActivity"
            android:label="NormalHolderActivity">
        </activity>
        <activity
            android:name=".activities.ExpandableListActivity_"
            android:label="NormalHolderActivity">
        </activity>
    </application>

</manifest>
